<block s-if="{{!disabled}}"><view class="u-upload data-v-0d33af60"><block s-for="{{lists}}" s-for-item="item" s-for-index="index" s-key="index"><block s-if="{{showUploadList}}"><view class="u-list-item u-preview-wrap data-v-0d33af60" style="{{'width:'+($root.g0)+';'+('height:'+($root.g1)+';')}}"><block s-if="{{deletable}}"><view data-event-opts="{{[['tap',[['deleteItem',[index]]]]]}}" class="u-delete-icon data-v-0d33af60" style="{{'background:'+(delBgColor)+';'}}" catchtap="__e"><u-icon class="u-icon data-v-0d33af60" vue-id="{{'cd7e3212-1-'+index}}" name="{{delIcon}}" size="20" color="{{delColor}}"></u-icon></view></block><block s-if="{{showProgress&&item.progress>0&&!item.error}}"><u-line-progress class="u-progress data-v-0d33af60" vue-id="{{'cd7e3212-2-'+index}}" show-percent="{{false}}" height="16" percent="{{item.progress}}"></u-line-progress></block><block s-if="{{item.error}}"><view data-event-opts="{{[['tap',[['retry',[index]]]]]}}" class="u-error-btn data-v-0d33af60" catchtap="__e">点击重试</view></block><block s-if="{{!item.isImage}}"><image class="u-preview-image data-v-0d33af60" src="{{item.url||item.path}}" mode="{{imageMode}}" data-event-opts="{{[['tap',[['doPreviewImage',[item.url||item.path,index]]]]]}}" catchtap="__e"></image></block></view></block></block><slot name="file" var-file="lists"></slot><block s-if="{{maxCount>lists.length}}"><view data-event-opts="{{[['tap',[['selectFile',['$event']]]]]}}" style="display:inline-block;" bindtap="__e" class="data-v-0d33af60"><slot name="addBtn"></slot><block s-if="{{!customBtn}}"><view class="u-list-item u-add-wrap data-v-0d33af60" style="{{'width:'+($root.g2)+';'+('height:'+($root.g3)+';')}}" hover-class="u-add-wrap__hover" hover-stay-time="150"><u-icon class="u-add-btn data-v-0d33af60" vue-id="cd7e3212-3" name="plus" size="40"></u-icon><view class="u-add-tips data-v-0d33af60">{{uploadText}}</view></view></block></view></block></view></block>