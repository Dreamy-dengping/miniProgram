<view class="w-picker-view"><picker-view class="d-picker-view" indicator-style="{{itemHeight}}" value="{{pickVal}}" data-event-opts="{{[['change',[['handlerChange',['$event']]]]]}}" onChange="__e"><picker-view-column><view class="w-picker-item" a:for="{{range.provinces}}" a:for-item="item" a:for-index="index" a:key="index">{{item.label}}</view></picker-view-column><picker-view-column><view class="w-picker-item" a:for="{{range.citys}}" a:for-item="item" a:for-index="index" a:key="index">{{item.label}}</view></picker-view-column><block a:if="{{!hideArea}}"><picker-view-column><view class="w-picker-item" a:for="{{range.areas}}" a:for-item="item" a:for-index="index" a:key="index">{{item.label}}</view></picker-view-column></block></picker-view></view>