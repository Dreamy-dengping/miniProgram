<block a:if="{{dataBuffer}}"><view class="goodlist"><view class="head"><view class="search"><u-search vue-id="269970b6-1" disabled="{{true}}" shape="square" show-action="{{true}}" action-text="搜索" animation="{{true}}" margin="10rpx 0rpx" data-event-opts="{{[['^click',[['goSearch']]]]}}" onClick="__e" onVueInit="__l"></u-search></view><view class="sortbox"><view class="sort"><view hover-class="touchOpacity">综合排序</view><view hover-class="touchOpacity">销量</view><view hover-class="touchOpacity" data-event-opts="{{[['tap',[['pricesort',['$event']]]]]}}" onTap="__e"><text>价格</text><image src="../../static/icon/sort.png"></image></view><view hover-class="touchOpacity">好评度</view><view hover-class="touchOpacity">店铺</view></view><view class="filter" hover-class="touchOpacity"><image src="../../static/icon/filter.png"></image><text>筛选</text></view></view></view><view class="biaoyu" hover-class="touchOpacity"><u-notice-bar vue-id="269970b6-2" mode="vertical" list="{{list}}" show="{{true}}" close-icon="{{true}}" onVueInit="__l"></u-notice-bar></view><view class="list"><view data-event-opts="{{[['tap',[['goDesc',['$0'],[[['goodlist','',index,'id']]]]]]]}}" class="row" onTap="__e" a:for="{{goodlist}}" a:for-item="item" a:for-index="index" a:key="index"><view class="image"><u-lazy-load vue-id="{{'269970b6-3-'+index}}" image="{{imgUrl+item.img}}" height="218" onVueInit="__l"></u-lazy-load></view><view class="info" hover-class="touchOpacity"><text>{{item.goodsname}}</text><view class="money"><text>{{"￥"+item.price}}</text><text>{{"￥"+item.market_price}}</text></view><text>双11.11限制200起</text><text space="ensp">3565评论   99%好评</text></view></view></view><block a:if="{{topBuffer}}"><view class="quick_top" hover-class="touchOpacity" data-event-opts="{{[['tap',[['goTop',['$event']]]]]}}" onTap="__e"><image src="../../static/icon/totop.png"></image></view></block></view></block><block a:else><view><u-empty vue-id="269970b6-4" text="暂无数据~~去逛逛~~" mode="list" show="{{!dataBuffer}}" color="oran