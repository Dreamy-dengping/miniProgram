.login_box{
		height: 100%;
		background-color: #f2f2f2;
		overflow: hidden;
}
.text_login{
		background-color: #FFFFFF;
		margin: 20rpx 20rpx 0;
		height: 100vh;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.login_logo{
		width: 364rpx;
		height: 96rpx;
		margin: 488rpx 0 88rpx 0;
}
.login_phone{
		height: 60rpx;
		width: 610rpx;
		border-bottom: 1rpx solid #CCCCCC;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		color: #666666;
		font-size: 24rpx;
}
input{
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		margin-left: 15rpx;
}
.login_code{
		height: 60rpx;
		width: 610rpx;
		border-bottom: 1rpx solid #CCCCCC;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		color: #666666;
		font-size: 24rpx;
		margin-top: 34rpx;
}
.login_code view{
		width: 160rpx;
		line-height: 48rpx;
		height: 48rpx;
		text-align: center;
		background-color: #CCCCCC;
		font-size: 20rpx;
		color: #FFFFFF;
}
.login_but{
		width:610rpx;
		line-height: 49px;
		background-color: #CCCCCC;
		text-align: center;
		font-size: 32rpx;
		color: #FFFFFF;
		margin-top: 40rpx;
}

