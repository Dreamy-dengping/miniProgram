<view class="data-v-db675620"><block a:for="{{siteList}}" a:for-item="res" a:for-index="index" a:key="id"><block a:if="{{dataBuffer}}"><view class="item data-v-db675620"><u-icon vue-id="{{'91966c28-1-'+index}}" name="close-circle-fill" size="{{40}}" color="#ff5500" index="{{res.id}}" data-event-opts="{{[['^click',[['del']]]]}}" onClick="__e" class="data-v-db675620" onVueInit="__l"></u-icon><view data-event-opts="{{[['tap',[['backorder',[index]]]]]}}" class="top data-v-db675620" onTap="__e"><view class="name data-v-db675620">{{res.username}}</view><view class="phone data-v-db675620">{{res.userphone}}</view><view class="tag data-v-db675620"><block a:if="{{res.status==1}}"><text class="red data-v-db675620">默认</text></block></view></view><view class="bottom data-v-db675620">{{''+(res.location+res.useraddress)+''}}<u-icon vue-id="{{'91966c28-2-'+index}}" name="edit-pen" size="{{40}}" color="#999999" index="{{res.id}}" data-event-opts="{{[['^click',[['edit']]]]}}" onClick="__e" class="data-v-db675620" onVueInit="__l"></u-icon></view></view></block><block a:else><view class="data-v-db675620">暂无收货地址信息!</view></block></block><view data-event-opts="{{[['tap',[['toAddSite',['$event']]]]]}}" class="addSite data-v-db675620" onTap="__e"><view class="add data-v-db675620"><u-icon class="icon data-v-db675620" vue-id="91966c28-3" name="plus" color="#ffffff" size="{{30}}" onVueInit="__l"></u-icon>新建收货地址</view></view></view>