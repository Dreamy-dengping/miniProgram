/* 秒杀热推区域 */
.seckill {
		height: 350rpx;
		background-color: #f1f1f1;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		margin: 20rpx 20rpx;
}
/* 秒杀热推左侧区域 */
.seckill .leftseckill {
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		border-right: 1rpx solid #F1F1F1;
		background-color: white;
		background: url(data:image/png;base64,) no-repeat;
		background-size: cover;
}
.leftseckill .title {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.left_seckill_info{
		margin-left: 20rpx;
}
.seckill_img{
		width: 180rpx;
		height: 180rpx;
		border-radius: 100%;
		border: 1rpx solid #ff6040;
		margin:50rpx  16rpx ;
}
.title text:nth-of-type(1){
		color: #85a642;
		font-weight: bold;
		margin: 20rpx 0 10rpx 20rpx;
}
.title text:nth-of-type(2){
		font-size: 15rpx;
		color: #555555;
		margin: 20rpx 20rpx 0 0;
}
/* 秒杀热推右侧区域 */
.seckill .rightseckill {
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		margin-left: 10rpx;
		overflow: hidden;
}
.rightseckill view {
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
}
.rightseckill .top{
		background: url(data:image/png;base64,) no-repeat;
		background-size: cover;
}
.rightseckill .top view:nth-of-type(1){
		color: #ff6040;
		font-weight: bold;
		margin: 20rpx 0 10rpx 20rpx;
}
.rightseckill .top view:nth-of-type(2){
		color: #ff6040;
		font-size: 15rpx;
		margin: 20rpx 0 0 20rpx;
}
.rightseckill .top view:nth-of-type(3){
		color: #ff6040;
		font-size: 15rpx;
		color: white;
		margin: 10rpx 0 0 20rpx;
		background: -webkit-linear-gradient(#ff6040,#ff9878);
		background: linear-gradient(#ff6040,#ff9878);
		border-top-right-radius: 30rpx;
		border-bottom-right-radius: 30rpx;
		width:125rpx;
		line-height: 40rpx;
		text-align: center;
}
.rightseckill .bottom {
		margin-top: 10rpx;
		background: url(data:image/png;base64,)no-repeat;
		background-size: cover;
}
.rightseckill .bottom view:nth-of-type(1){
		color: #af40ff;
		font-weight: bold;
		margin: 20rpx 0 10rpx 20rpx;
}
.rightseckill .bottom view:nth-of-type(2){
		color: #af40ff;
		font-size: 15rpx;
		margin: 20rpx 0 0 20rpx;
}
.rightseckill .bottom view:nth-of-type(3){
		color: #af40ff;
		font-size: 15rpx;
		color: white;
		margin: 10rpx 0 0 20rpx;
		background: -webkit-linear-gradient(#af40ff,#ef85ff);
		background: linear-gradient(#af40ff,#ef85ff);
		border-top-right-radius: 30rpx;
		border-bottom-right-radius: 30rpx;
		width:125rpx;
		line-height: 40rpx;
		text-align: center;
}
.rightseckill .opacity{
		opacity: 0.3;
}
