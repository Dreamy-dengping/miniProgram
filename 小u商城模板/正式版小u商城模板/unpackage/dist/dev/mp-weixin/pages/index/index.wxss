/* 推荐商品详情 */
.recommendlist {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		margin-top:20rpx;
}
.recommendlist .recommendrow {
		margin: 0 20rpx 20rpx 20rpx;
		height: 325rpx;

		box-shadow: 5rpx 10rpx 5rpx #3333;

		border: 1rpx solid #DDD;
		position: relative;
}
.recommendrow  .image {
		position: absolute;
		top: 65rpx;
		left: 20rpx;
		width: 200rpx;
		height: 200rpx;
}
.recommendrow .recommendrowinfo {
		position: absolute;
		height: 210rpx;
		top: 60rpx;
		left: 240rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-justify-content: space-around;
		        justify-content: space-around;
}
.recommendrowinfo text:nth-of-type(1) {
		color: black;
}
.recommendrowinfo text:nth-of-type(2) {
		color: red;
}
.recommendrowinfo text:nth-of-type(3) {
		color: #ccc;
		text-decoration: line-through;
		font-size: 24rpx;
}
.recommendrowinfo text:nth-of-type(4) {
		color: white;
		background-color: #ffa500;
		border-radius: 10rpx;
		width: 160rpx;
		line-height: 55rpx;
		text-align: center;
		font-size: 28rpx;
}
.recommendnav .recommendactive {
		background-color: #FF6040;
		color: white;
}
